        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Boss infomation (Spellcard name Animation)
draw_text_outline_coloured(202, 65+NameMove, string(global.BossName), c_white, c_white, WhiteTextGrad, WhiteTextGrad, c_black, 1, ft_YuGothicSpellcard, NameFade)

if global.SpellHUDdisplay = true
{
    if NameFade &lt; 1 //Fades in the Boss's name
    {
        NameFade += 1/30
    }
      
    if global.SpellcardType = "Spellcard" or global.SpellcardType = "TimeOut"
     {
        if BNMD &lt; 100 //Boss name Movement Delay
        {
            BNMD += 1
        }
        else
        {
            NMA = 50 //Name Move Ammount
            if NameMove &lt; NMA //Moves spellcard when spells are active
            {
                NameMove += NMA/20
            }   
        }
        
       draw_sprite_ext(spr_SpellcardLine, 0, BossHPX, BossHPY + 40, 1, 1, 0, c_white, SpellLineAlpha) 
       draw_text_outline_coloured(BossHPX+5, BossHPY + 47, string(global.Spellcardname), c_white, c_white, WhiteTextGrad, WhiteTextGrad, c_black, 1, ft_YuGothicSpellcard1, SpellLineAlpha + SpellnameAlpha)
       
       //Spell Bonus
       SpellBonus = string_repeat("0", 7 - string_length(string(floor(global.SpellBonus)))) + string(global.SpellBonus)
       for (var i = string_length(SpellBonus) - 2; i &gt; 1; i -= 3)
           {
           SpellBonus = string_insert(",", SpellBonus, i);
           }
           
       if global.SpellCaptured = true
          {
            draw_text_outline_coloured(BossHPX + 130, BossHPY + 80, "Bonus:  " + string(SpellBonus), c_white, c_white, WhiteTextGrad, WhiteTextGrad, c_black, 1, ft_YuGothicSpellcard, SpellLineAlpha)
          }
          else
          {
            draw_text_outline_coloured(BossHPX + 130, BossHPY + 80, "Bonus:  " + "Failed...", c_white, c_white, failed, failed, c_black, 1, ft_YuGothicSpellcard, SpellLineAlpha)
          }
          
       if SpellLineDisplay = true
       {
            if NameMove = NMA
            {
                if SpellLineAlpha &lt; 1
                {
                    SpellLineAlpha += 1/20
                }
            }
       }
       else
       {
            if NameMove &gt; 0 // Moves Boss name back when spellcard is inactive
            {
                NameMove -= NMA/10
                BNMD = 0
            } 
            
       }
     }
     else
     {
        SpellLineAlpha = 0
     }
}
else
{
    if NameFade &gt; 0 //Fades out the Boss's name
    {
        NameFade -= 1/30
    }
}
