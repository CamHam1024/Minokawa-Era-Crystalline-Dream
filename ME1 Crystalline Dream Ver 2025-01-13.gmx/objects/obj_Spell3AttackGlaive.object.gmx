        hspeed = -hspeed
        x = 200
        Rotate =! Rotate 
        speed += SpeedIncrease
    }
    
    if x &gt; 1024-190
    {
        hspeed = -hspeed
        x = 824
        Rotate =! Rotate 
        speed += SpeedIncrease
    }
    
    if y &gt; 768+14
    {
        vspeed = -vspeed
        y = 768-15
        speed += SpeedIncrease
    }
    
    if y &lt; 14
    {
        vspeed = -vspeed
        y = 15
        speed += SpeedIncrease
    }
}

