        //Attack
        if Type = 0
        {
            //= Settingsrrr =========  
            shotnum = ShotNum  //Number of bullets
            shotdir = direction //Direction of the burst
            rad = 0 //distence from the Center point
            fireX = x //Startx
            fireY = y //StartY
            shotspeed = 3 //speed of bullet
            shotspread = 360 //Angle of burst (360 = full circle)
            shotdevi = shotspread/shotnum
            shotobject = obj_AlexRegular1NEW //Bullet
           
             //-=[Bullet Spawner]=-
             shotdir -= shotspread / 2
             repeat shotnum 
             {
                b = instance_create(fireX + lengthdir_x(rad, shotdir) ,fireY + lengthdir_y(rad, shotdir),shotobject)   
                b.direction = shotdir
                b.Type = 0
                b.ImageIndex = 9
                b.MaxSpeed = shotspeed
                
                b1 = instance_create(fireX + lengthdir_x(rad, shotdir) ,fireY + lengthdir_y(rad, shotdir),shotobject)   
                b1.direction = shotdir + shotdevi/2
                b1.Type = 1
                b1.ImageIndex = 11
                b1.MaxSpeed = shotspeed/2
                
                
                shotdir += shotspread/(shotnum)
             }
        }
        
        if Type = 1
        {
            //= Settingsrrr =========  
            shotnum = ShotNum  //Number of bullets
            shotdir = direction //Direction of the burst
            rad = 0 //distence from the Center point
            fireX = x //Startx
            fireY = y //StartY
            shotspeed = 3 //speed of bullet
            shotspread = 360 //Angle of burst (360 = full circle)
            shotdevi = shotspread/shotnum
            shotobject = obj_AlexRegular1NEW //Bullet
           
             //-=[Bullet Spawner]=-
             shotdir -= shotspread / 2
             repeat shotnum 
             {
                b = instance_create(fireX + lengthdir_x(rad, shotdir) ,fireY + lengthdir_y(rad, shotdir),shotobject)   
                b.direction = shotdir
                b.Type = 1
                b.ImageIndex = 11
                b.MaxSpeed = shotspeed
                
