    Phase = 1
}

if Phase = 1
{
    if BlinkSpeed &lt; 3
    {
        Effect = instance_create(x, y, obj_EnemyBurstEffect)
        Effect.R = Red
        Effect.G = Green
        Effect.B = Blue
        Effect.image_xscale = 2
        Effect.image_yscale = 2
        
