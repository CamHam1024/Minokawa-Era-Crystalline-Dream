          <argument>
            <kind>1</kind>
            <string>///Intro
if IntroPlayed = false
{
    draw_sprite_ext(spr_Blackbacker, 0, 0, 0, 1, 1, 0, c_white, IntroBackgroundAlpha-0.3)

    draw_sprite_ext(spr_Logo,0, 1024/2-100, 768/2-50, 0.7, 0.7, 0, c_white, IntroAlpha)
    //draw_text_fancy_colour(1024/2+60, 768/2+145, "Presents...", make_colour_hsv(164, 174, 217), ft_DokiTounge, IntroAlpha1-0.5)
    draw_text_fancy_colour(1024/2+50, 768/2+125, "Presents...", make_colour_hsv(141, 149, 216), ft_MenuTextLG, IntroAlpha1)   

    if IntroPhase = 0
    {
        IntroBackgroundAlpha = 0.7
        if IntroAlpha &lt; 1
        {
            IntroAlpha += 1/60
        }
        else
        {
            IntroTimer ++
            if IntroTimer = 30
            {
                IntroPhase = 1
                IntroTimer = 0
            }
        }
        
    }

    if IntroPhase = 1
    {
        if IntroAlpha1 &lt; 1
        {
            IntroAlpha1 += 1/60
        }
        else
        {
            IntroTimer ++
            if IntroTimer = 60
            {
                IntroPhase = 2
                FullClose = false
                instance_create(0, 0, obj_MainMenu)
                
                if ds_grid_get(global.OstUnlocked, 2, 0) = false
                {
                    instance_create(x, y, obj_NewDiaryEntry)
                    ds_grid_set(global.OstUnlocked, 2, 0, true)
        
                    //Save Diary Entry
                    ini_open("Settings.ini")

                    ini_write_string("Diary", "MusicRecords", ds_grid_write(global.OstUnlocked))
        
                    ini_close()
                }
                
                IntroTimer = 0 
            }
        }
    }

    if IntroPhase = 2
    {
        if IntroBackgroundAlpha &gt; 0
        {
            IntroBackgroundAlpha -= 0.7/30
            IntroAlpha -= 1/30
            IntroAlpha1 -= 1/30
        }
        else
        {
            IntroPlayed = true
        }
    }
}

if IntroPlayed = true
{
    if room = room_MainMenu
    {
        if !instance_exists(obj_MainMenu)
        {
            instance_create(0, 0, obj_MainMenu)
        }
    }
}
