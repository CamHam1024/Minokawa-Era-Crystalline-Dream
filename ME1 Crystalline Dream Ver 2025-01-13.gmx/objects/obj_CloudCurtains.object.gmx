        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Main event
Number = 10
Scale = 0.6

Spindirection += 0.01
Spindirection = Spindirection % 360
Spindirection1 += 0.05
Spindirection1 = Spindirection1 % 360

repeat(Number)
{
    CloudRingX = lengthdir_x(Length+110,Direction+Spindirection1)
    CloudRingY = lengthdir_y(Length+110,Direction+Spindirection1)
    
    draw_sprite_ext(spr_PinkMenuCloud, 0, 512+CloudRingX, 384+CloudRingY, Scale, Scale, Direction+Spindirection, c_white, 1)
    Direction += 360/Number
}
Direction = 0

repeat(Number+2)
{
    CloudRingX = lengthdir_x(Length+350,Direction-Spindirection)
    CloudRingY = lengthdir_y(Length+350,Direction-Spindirection)
    
    draw_sprite_ext(spr_PinkMenuCloud, 0, 512+CloudRingX, 384+CloudRingY, Scale, Scale, Direction-Spindirection, make_color_hsv(153,30,255), 1)
    Direction += 360/Number+2
}
Direction = 0

repeat(Number+10)
{
    CloudRingX = lengthdir_x(Length+510,Direction+Spindirection)
    CloudRingY = lengthdir_y(Length+510,Direction+Spindirection)
    
    draw_sprite_ext(spr_PinkMenuCloud, 0, 512+CloudRingX, 384+CloudRingY, Scale, Scale, Direction+Spindirection, make_color_hsv(153,60,255), 1)
    Direction += 360/Number+10
}
Direction = 0

repeat(Number+12)
{
    CloudRingX = lengthdir_x(Length+640,Direction-Spindirection1)
    CloudRingY = lengthdir_y(Length+640,Direction-Spindirection1)
    
    draw_sprite_ext(spr_PinkMenuCloud, 0, 512+CloudRingX, 384+CloudRingY, Scale, Scale, Direction-Spindirection, make_color_hsv(153,90,255), 1)
    Direction += 360/Number+12
}
Direction = 0


//===========
if FullClose = true
{
    if Length &gt; 0
    {
        Length -= 1000/40
    }
}
else
{
    if Length &lt; 1000
    {
        Length += 1000/30
    }
}
</string>
