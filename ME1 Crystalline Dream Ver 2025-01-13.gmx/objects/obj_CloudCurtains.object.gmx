        Timer += 1
        FullClose = true
    }
    else
    {
        Timer = 0
        FullCloseTransiton = false
        FullClose = false
        room_goto(room_Stage)
    }
}

if GoBackTrans = true
{
    if Timer &lt; 60
    {
        Timer += 1
        FullClose = true
    }
    else
    {
        Timer = 0
        GoBackTrans = false
        FullClose = false
        room_goto(room_MainMenu)
        instance_activate_all()
        global.Pause = false
        global.Continues = 9
        global.ContinuePoint = 0
        instance_activate_object(obj_MusicHandler)
        obj_MusicHandler.TrackNo = 0
        obj_MusicHandler.MusicReset = true
        obj_MusicHandler.MusicPaused = true
        audio_stop_all()
        audio_resume_all()
    }
}


