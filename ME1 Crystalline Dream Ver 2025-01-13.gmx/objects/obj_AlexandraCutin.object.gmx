//Init
if (!surface_exists(CutinAnimation)) 
{
  CutinAnimation = surface_create(800, 800)
}

width = surface_get_width(CutinAnimation)
height = surface_get_height(CutinAnimation)

XPos = width/2
YPos = height/2

//Banner===========

if Phase &lt; 2
{
    if BannerRot &lt; 10 
    {
        BannerRot += 10/300
    }
}


//=================
draw_sprite_ext(spr_AlexandraBanner,0, 512+100,768/2, 1, BannerScale, 70+BannerRot, c_white, 1)
draw_sprite_ext(spr_AlexandraBanner,0, 512,368+150, 1, BannerScale, -10-BannerRot, c_white, 1)

surface_set_target(CutinAnimation)
draw_clear_alpha(0,0)

//Draw
if Phase = 0
{
    if SurfAlpha &lt; 1
    {
        BannerScale += 1/10
        SurfAlpha += 1/10
        SurfScale += 0.1/10
        
        MoveMain -= 50/10
    }
    else
    {
        Phase = 1
    }
}

if PotMov &lt; 25
{
    PotMov += 25/200
}
PotRot += 0.1

if Phase = 1
{
    MoveMain -= 0.3
    
    if PhaseTimer &lt; 110
    {
        PhaseTimer +=1
    }
    else
    {
        Phase = 2
    }
    
    if BackAlpha &gt; 0
    {
        BackAlpha -= 1/10
        BScale += 0.5/10
    }
    
    if ScarfRot &lt; 10 { ScarfRot += 10/80 }
    if LanternRot &gt; -10 { LanternRot -= 10/80 }
    if ArmRot &lt; 10 
    { 
        ArmRot += 10/80 
    }
    
    if PotScale &lt; 1 {PotScale += 0.5/200}
}

if Phase = 2
{
    if SurfAlpha &gt; 0
    {
        SurfAlpha -= 1/10
        if BannerScale &gt; 0
        {
            BannerScale -= 1/5
        }
        MoveMain -= 10
    }
    else
    {
        instance_destroy()
    }
}
draw_set_blend_mode(bm_add)
draw_sprite_ext(spr_AlexFullNoPot, 1, XPos+9, YPos-2, BScale, BScale, 0, c_white, BackAlpha)
