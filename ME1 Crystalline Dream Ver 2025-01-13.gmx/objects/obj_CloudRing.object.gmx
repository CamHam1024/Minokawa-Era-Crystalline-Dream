SpinDir = 0

ActiveSpin += 0.5
ActiveSpin = ActiveSpin % 360

if instance_exists(obj_ParPlayer)
{
    x = obj_ParPlayer.x
    y = obj_ParPlayer.y
}

if global.DreamBoarderActivate = true
{
    if RingOneLen &lt; 100
    {
        RingOneLen += 100/30
        RingTwoLen += 50/30
    }
}
else
{
    if CloudAlpha &gt; 0
    {
        CloudAlpha -= 1/60
        RingOneLen -= 100/60
        RingTwoLen -= 50/60
    }
    else
    {
        instance_destroy()
    }
}

//draw_sprite_ext(spr_CircleCloud, 0, x, y, 1, 1, ActiveSpin, c_white, CloudAlpha-0.7)
    
repeat(RingOneNum)
{
    RingX = x + lengthdir_x(RingOneLen,SpinDir+ActiveSpin)
    RingY = y + lengthdir_y(RingOneLen,SpinDir+ActiveSpin)

    draw_sprite_ext(spr_MechanicCLoud, 0, RingX, RingY, 1, 1, SpinDir-ActiveSpin, c_white, CloudAlpha-0.7)
    
    SpinDir += 360/RingOneNum
}

repeat(RingOneNum)
{
    RingX = x + lengthdir_x(RingTwoLen,SpinDir-ActiveSpin)
    RingY = y + lengthdir_y(RingTwoLen,SpinDir-ActiveSpin)

    draw_sprite_ext(spr_MechanicCLoud, 0, RingX, RingY, 1, 1, SpinDir+ActiveSpin, c_white, CloudAlpha-0.7)
    
    SpinDir += 360/RingOneNum
}
