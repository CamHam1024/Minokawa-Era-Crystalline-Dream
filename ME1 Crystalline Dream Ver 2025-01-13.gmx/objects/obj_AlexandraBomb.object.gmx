event_inherited()

//Events
if x &lt;= 192
{
    hspeed = -hspeed
    x += 1
}

if x &gt;= 1024 - 192
{
    hspeed = -hspeed
    x -= 1
}

if y &lt;= 16
{
    vspeed = -vspeed
    y += 1
}

if y &gt;= 768 - 16
{
    vspeed = -vspeed
    y -= 1
}

if speed &gt; 0
{
    speed -= StarSpeed/SpeedTimer
}

if speed = 0
{
    Timer += 1
    if Timer = 10
    {
        basic_bullet_burst(obj_AlexandraFireballSP1, FireNumb, direction, 10, x, y, FireballSpeed, 0, 360)

        sound = audio_play_sound(snd_SFX_EnemyDead, 0, 0)
        audio_sound_gain(sound, global.VolSFX, 0)
        
        if global.Diff = "Flightless"
        {
            //basic_bullet_burst(obj_SpellAlexStar3, FireNumb/2, direction/FireNumb/2, -20, x, y, 2, 0, 360)
        }
        
        Blast = instance_create(x, y, obj_AlexBlastEmitter)
        Blast.direction = direction
        
        Effect = instance_create(x, y, obj_EnemyBurstEffect)
        Effect.R = Red
        Effect.G = Green
        Effect.B = Blue
        Effect.image_xscale = 2
        Effect.image_yscale = 2
    
        AttackKilled = true
        instance_destroy()
    }
}
