            <string>///Properties
//Killers
event_inherited()

//Events
if image_xscale &gt; 0.3
{
    Deadly = true
}

if Killed = false
{

    if ChainHP &gt; 0
    {
        if image_xscale &lt; 1
        {
            image_xscale += 1/10
        }
    }
    else
    {
        if Burst = false
        {
            //Burst wave
            Effect = instance_create(x, y, obj_EnemyBurstEffect)
            Effect.R = 200
            Effect.G = 200
            Effect.B = 200
            Effect.image_xscale = 1
            Effect.image_yscale = 1
            
            Burst = true
            basic_bullet_burst(obj_MiddySpell2Danmaku1, 1, random(360), 10, x, y, 3, 0, 360)
    
        }
        
        if image_xscale &gt; 0
        {
            image_xscale -= 1/10
        }
        
        if DeadTimer &lt; 120
        {
            DeadTimer += 1
        }
        else
        {
            ChainHP = 20
            Burst = false
            DeadTimer = 0
        }
    }
}
else
{
    if Redshift = true
    {
        ExplodeSpeed = 60
        
        if RandomWabble &lt; 10
        {
            x = startx + random_range(-RandomWabble,RandomWabble)  
            y = starty + random_range(-RandomWabble,RandomWabble) 
            RandomWabble += 10/ExplodeSpeed
        }
        
        image_blend = make_colour_rgb(150,ColourShift,ColourShift)
        
        if ColourShift &gt; 0
        {
            ColourShift -= 255/ExplodeSpeed
        }
        else
        {
            repeat(fragnumber)
            {
                frag = instance_create(x, y, obj_MiddySpell2Danmaku4)
                frag.direction = random(360)
            }
            PointInDirection = false
            Deadly = false
            AttackKilled = true
            instance_create(x, y, obj_deatheffectcahin)
            instance_destroy()
        }
    }
    else
    {
        PointInDirection = false
        Deadly = false
        AttackKilled = true
        instance_create(x, y, obj_deatheffectcahin)
        instance_destroy()
    }
}

</string>
