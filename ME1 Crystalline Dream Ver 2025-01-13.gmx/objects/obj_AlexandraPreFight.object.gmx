            <string>///Dialog Scroll
DialogLength = array_length_1d(Dialog) - 1

if ForcePause = false
{
    if keyboard_check(vk_control)
    {
        AutoScroll = true
        AutotimerMax = 3 
    }

    if keyboard_check_released(vk_control)
    {
        AutoScroll = false 
    }
}


if AutoScroll = false
{
    if keyboard_check_pressed(ord("Z"))
    {
        if n &lt; DialogLength
        {
            n += 1
        }
        EventSwitch = false
    }
}
else
{
    if AutoTimer &lt; AutotimerMax
    {
        AutoTimer += 1
    }
    else
    {
        if n &lt; DialogLength
        {
            n += 1
        }
        EventSwitch = false
        AutoTimer = 0
    }
}
</string>
