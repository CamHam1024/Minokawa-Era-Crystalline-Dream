            <kind>1</kind>
            <string>repeat(1)
{
    Play_Sound_Fancy(snd_SFX_BulletFired1,global.VolSFX)
    
    ExRand = random_range(20, 160)
    ExploEmitX = x + lengthdir_x(random(BlastRadius),ExRand) //lengthdir_x(radius,Direction)
    ExploEmitY = y + lengthdir_y(random(BlastRadius),ExRand)
    
    if BlastRadius &lt; MaxBlastRadius
    {
        BlastRadius += MaxBlastRadius/ExploTime
    }
    
    Blast = instance_create(ExploEmitX, ExploEmitY, obj_ShouBlastEffect)
    Blast.image_blend = Blend
    
    if global.Diff = "Hard"
    {
        Bullet = instance_create(ExploEmitX, ExploEmitY, obj_ShouPackageDanmakuHard)
        Bullet.direction = random_range(20, 160)
        Bullet.MaxSpeed = 3
        //Bullet.image_blend = make_colour_hsv(Blend, 255, 255)
    }
    
    if global.Diff = "Flightless"
    {
        Bullet = instance_create(ExploEmitX, ExploEmitY, obj_ShouPackageDanmakuFlight)
        Bullet.direction = random_range(20, 160)
        Bullet.MaxSpeed = 3
    }
}

if Time &lt; 30
{
    Time += 1
}
else
{
    instance_destroy()
}



</string>
