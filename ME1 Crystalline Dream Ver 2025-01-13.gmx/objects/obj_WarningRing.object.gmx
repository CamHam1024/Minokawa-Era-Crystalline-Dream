            <string>///Primatives

//Init
var i
RadiusAlter = 0

xx = x    
yy = y

TexStartx = (mouse_x-radius)/room_width //surface_get_width(global.backside)
TexStarty = (mouse_y-radius)/room_height //surface_get_height(global.backside)

TexEndx = (mouse_x+radius)/room_width
TexEndy = (mouse_y+radius)/room_height

//Primative 
TEX = surface_get_texture(SpellBack)

draw_primitive_begin_texture(pr_trianglestrip, TEX)

draw_enable_alphablend(Blending)
xAlt = xx + lengthdir_x(minradius, ANGLE * 0 / steps + StarAngle)
yAlt = yy + lengthdir_y(minradius, ANGLE * 0 / steps + StarAngle)

draw_vertex_texture_colour(xx + lengthdir_x(minradius, StarAngle), yy + lengthdir_y(minradius, StarAngle), xAlt/room_width, yAlt/room_height, c_white, Suralpha)

for(i = 0; i &lt;= steps; i += 1)
{
    //draw_vertex(xx + lengthdir_x(radius, ANGLE  * i / steps), yy + lengthdir_y(radius, ANGLE  * i / steps))
    
    if RadiusAlter = 0
    {
        xAlt = xx + lengthdir_x(radius, ANGLE * i / steps + StarAngle)
        yAlt = yy + lengthdir_y(radius, ANGLE * i / steps + StarAngle)
    }
    else
    {
        xAlt = xx + lengthdir_x(minradius, ANGLE * i / steps + StarAngle)
        yAlt = yy + lengthdir_y(minradius, ANGLE * i / steps + StarAngle)
    }
    
    draw_vertex_texture_colour(xAlt, yAlt, xAlt/room_width, yAlt/room_height, c_white, Suralpha)
    RadiusAlter =! RadiusAlter
}

xAlt = xx + lengthdir_x(minradius, StarAngle)
yAlt = yy + lengthdir_y(minradius, StarAngle)
draw_vertex_texture_colour(xAlt, yAlt, xAlt/room_width, yAlt/room_height, c_white, Suralpha)
xAlt = xx + lengthdir_x(radius, StarAngle)
yAlt = yy + lengthdir_y(radius, StarAngle)
draw_vertex_texture_colour(xAlt, yAlt, xAlt/room_width, yAlt/room_height, c_white, Suralpha)

draw_primitive_end()
draw_enable_alphablend(true)






