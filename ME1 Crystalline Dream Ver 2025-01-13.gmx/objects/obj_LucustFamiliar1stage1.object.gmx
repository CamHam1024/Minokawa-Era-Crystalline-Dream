        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Attack

if !instance_exists(Host)
{
    EnemyHP = 0
}


if instance_exists(Host) and Host.FamKill = false
{
    DirectionSpin += 0.1
    DirectionSpin =DirectionSpin % 360

    if Burst &lt; BurstNum
    {
        FireSpeed = 2
        
        Fire += 1
        Fire = Fire % FireSpeed

        if Fire = 1
        {
            //= Settings =========  
            shotobject = obj_Lucust1stage1Danmaku1 //Bullet
            shotnum = BullNum  //Number of bullets
            shotdir = ovalrot //Direction of the burst
            rad = 10 //distence from the Center point
            fireX = x //Startx
            fireY = y //StartY
            shotspeed = 0 //speed of bullet
            shotspread = 360 //Angle of burst (360 = full circle)
            
            //-=[Bullet Spawner]=-
            shotdir -= shotspread / 2
            repeat shotnum 
            {
                b = instance_create(fireX + lengthdir_x(rad, shotdir) ,fireY + lengthdir_y(rad, shotdir),shotobject)   
                b.direction = shotdir
                b.MS = SpdInc
                b.ImageIndex = 7 + Burst
                
                if shotspread &lt; 360
                {  
                    shotdir += shotspread/(shotnum-1)
                }
                else
                {
                    shotdir += shotspread/(shotnum)
                }
            }
            
            SpdInc += 2/5
            Burst += 1
        }
    }
    else
    {
        if BurstTimer &lt; BTimerDuration
        {
            BurstTimer += 1
        }
        else
        {
            Burst = 0
            SpdInc = 3
            BurstTimer = 0
        }
    }
}
</string>
