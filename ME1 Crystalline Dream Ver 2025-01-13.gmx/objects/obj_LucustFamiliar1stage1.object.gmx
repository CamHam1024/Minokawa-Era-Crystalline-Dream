            <string>///Movement
//Base Movement
//EnemyMove()

//Custom Movement

if AltMove = false and instance_exists(Host)
{
    ovalrot += 0.5
    ovalrot = ovalrot % 360
    ovalrotS = dsin(ovalrot) //sine of the rotation angle (use sin instead of dsin if radians)
    ovalrotC = dcos(ovalrot) //sine of the rotation angle (use cos instead of dcos if radians)

    xfactor = lengthdir_x(xMoverad,OvalDir) //x and y position on basic oval
    yfactor = lengthdir_y(yMoverad,OvalDir)
    finalX = xfactor * ovalrotC - yfactor * ovalrotS //final x and y after rotation
    finalY = xfactor * ovalrotS + yfactor * ovalrotC

    x = Host.x + finalX
    y = Host.y + finalY

    //Custom movement adjustments
    OvalDir += OvalDirSpeed
    OvalDir = OvalDir % 360

    if xMoverad &lt; MaxXMoveRad
    {
        xMoverad += MaxXMoveRad/60
    }

    if yMoverad &lt; MaxYMoveRad
    {
        yMoverad += MaxYMoveRad/60
    }
}

